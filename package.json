  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MusicJunkyREZ/music-machine3/issues"
  },
  "homepage": "https://github.com/MusicJunkyREZ/music-machine3#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt-nodejs": "0.0.3",
    "connect-flash": "^0.1.1",
    "eslint-plugin-react": "^7.13.0",
    "express": "^4.17.1",
    "express-session": "^1.16.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.5.13",
    "morgan": "^1.9.1",
    "node-sass": "^4.12.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "colors": "^1.3.3",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9"
  }
}
